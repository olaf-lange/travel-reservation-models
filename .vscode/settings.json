{
  "mcp.servers": {
    "travel-reservations": {
      "command": "python",
      "args": [
        "${workspaceFolder}/mcp_server.py"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  },
  "chat.mcp.serverSampling": {
    "travel-reservation-models/.vscode/mcp.json: travel-reservations": {
      "allowedModels": [
        "copilot/gpt-4.1"
      ]
    }
  }
}
